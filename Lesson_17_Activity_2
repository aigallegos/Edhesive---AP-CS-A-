import java.util.Scanner;
import java.lang.Math;

public class Lesson_17_Activity_Two {
    public static void main (String[]args){
        Scanner scan = new Scanner (System.in);
        System.out.println("Enter two numbers: ");
        int a = scan.nextInt();
        int b = scan.nextInt();
        while (b >= a) {
            if ((a % 2) == 0) {
            System.out.print (" " + a);
            }
        a++;
        }
        
    }
}
